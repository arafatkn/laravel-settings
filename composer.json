{
    "name": "arafatkn/laravel-settings",
    "description": "A laravel package for managing project settings.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Arafat Islam",
            "email": "arafatkn@gmail.com"
        }
    ],
    "keywords": [
        "settings",
        "setting",
        "laravel settings"
    ],
    "autoload": {
        "psr-4": {
            "Arafatkn\\LaravelSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arafatkn\\LaravelSettings\\SettingServiceProvider"
            ],
            "aliases": {
                "LaravelSetting": "Arafatkn\\LaravelSettings\\Facades\\Setting"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
